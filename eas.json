{
  "cli": {
    "appVersionSource": "app.json"
  },
  "build": {
    "production": {
      "env": {
        "GOOGLE_SERVICE_PLIST": "@GOOGLE_SERVICE_PLIST"
      },
      "resourceClass": "m-medium",
      "autoIncrement": true,
      "distribution": "internal",
      "ios": {
        "simulator": false
      },
      "channel": "production",
      "hooks": {
        "pre-install": "./prebuild-ios.sh"
      }
    },
    "development": {
      "env": {
        "GOOGLE_SERVICE_PLIST": "@GOOGLE_SERVICE_PLIST"
      },
      "resourceClass": "m-medium",
      "autoIncrement": true,
      "distribution": "internal",
      "ios": {
        "simulator": false
      },
      "channel": "development",
      "hooks": {
        "pre-install": "./prebuild-ios.sh"
      }
    }
  },
  "submit": {
    "production": {}
  }
}

